<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="kr.co.codersit.launcherq.MainActivity">

    <RelativeLayout
        android:id="@+id/ac_main_rl_before_screen"
        android:background="#baffd2"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <Button
            android:id="@+id/ac_main_bt_testbtn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:text="Button" />

        <include
            android:id="@+id/ac_main_inc_basic_clock"
            layout="@layout/include_basic_clock1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="30dp" />

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/ac_main_rl_after_screen"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#84ffff00"
        android:visibility="invisible">

    </RelativeLayout>
</FrameLayout>
